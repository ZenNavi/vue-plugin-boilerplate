{
  "root": true,{{#flow}}
  "plugins": [
    "flowtype"
  ],{{/flow}}
  "extends": [
    "plugin:vue-libs/recommended"{{#flow}},
    "plugin:flowtype/recommended"{{/flow}}
  ],
  "rules": {
    "object-curly-spacing": ["error", "always"],
    "no-multiple-empty-lines": ["error", { "max": 2, "maxBOF": 1 }],
  }
}
